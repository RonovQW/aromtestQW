{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/admin", "dest": "/api/index.js" },
    { "src": "/api/admin-login", "dest": "/api/index.js" },
    { "src": "/api/admin-logout", "dest": "/api/index.js" },
    { "src": "/admin-login.html", "status": 308, "headers": { "Location": "/src/admin-login.html" } },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
